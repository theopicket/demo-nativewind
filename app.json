{
  "expo": {
    "name": "my-expo-app",
    "slug": "demo-nativewindui",
    "version": "1.0.0",
    "scheme": "my-expo-app",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.justshowcase.myexpoapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.justshowcase.myexpoapp"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7986f839-a001-460a-ac28-1488f9752280"
      }
    },
    "owner": "justshowcase"
  }
}
